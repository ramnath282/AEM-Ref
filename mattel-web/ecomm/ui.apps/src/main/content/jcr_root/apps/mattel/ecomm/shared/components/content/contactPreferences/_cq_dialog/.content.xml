<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Contact Preferences"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <contactPreferencesTitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Title for Contact Preference Page"
                        fieldLabel="Contact Preferences Title"
                        name="./contactPreferencesTitle"
                        required="{Boolean}true"/>
                    <emailAddressLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter label for email address"
                        fieldLabel="Email Address Label"
                        name="./emailAddressLabel"
                        required="{Boolean}true"/>
                    <emailPreferencesLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter label for Email Preferences"
                        fieldLabel="Email Preferences Label"
                        name="./emailPreferencesLabel"
                        required="{Boolean}true"/>
                    <emailPreferencesReceiveNotificationText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter text for email preferences receive notification"
                        fieldLabel="Email Preferences Receive Notification Text"
                        name="./emailPreferencesReceiveNotificationText"
                        required="{Boolean}true"/>
                    <emailPreferYes
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Prefer Yes Radio Button Label"
                        fieldLabel="Email Preferences Yes Radio Button Label"
                        name="./emailPreferYes"
                        required="{Boolean}true"/>
                    <emailPreferNo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Prefer No Radio Button Label"
                        fieldLabel="Email Preferences No Radio Button Label"
                        name="./emailPreferNo"
                        required="{Boolean}true"/>
                    <messagePreferencesLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter message preferences label"
                        fieldLabel="Message Preferences Label"
                        name="./messagePreferencesLabel"
                        required="{Boolean}true"/>
                    <messagePreferencesReceiveNotificationText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter text for message preferences receive notification"
                        fieldLabel="Message Preferences Receive Notification Text"
                        name="./messagePreferencesReceiveNotificationText"
                        required="{Boolean}true"/>
                    <receiveCatalougeLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter receive catalouge label"
                        fieldLabel="Receive Catalouge Label"
                        name="./receiveCatalougeLabel"
                        required="{Boolean}true"/>
                    <informationSharingLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter information sharing label"
                        fieldLabel="Information Sharing Label"
                        name="./informationSharingLabel"
                        required="{Boolean}true"/>
                    <updatePreferencesCTAText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter update preferences CTA text"
                        fieldLabel="Update Preferences CTA Text"
                        name="./updatePreferencesCTAText"
                        required="{Boolean}true"/>
                    <agRewardsLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter label for AG Rewards"
                        fieldLabel="AG Rewards Label"
                        name="./agRewardsLabel"
                        required="{Boolean}true"/>
                    <agRewardsReceiveNotificationText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter text for AG rewards receive notification"
                        fieldLabel="AG Rewards Receive Notification Text"
                        name="./agRewardsReceiveNotificationText"
                        required="{Boolean}true"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
