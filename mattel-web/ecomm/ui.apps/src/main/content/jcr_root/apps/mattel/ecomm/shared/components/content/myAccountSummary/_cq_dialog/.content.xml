<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Configure My Account Summary"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <welcomeConfig
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Welcome Section"
                        sling:resourceType="granite/ui/components/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <sectionTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Section Title"
                                fieldLabel="Section Title Label "
                                name="./sectionTitle"/>
                            <welcomeText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Welcome text"
                                fieldLabel="Welcome text Label "
                                name="./welcomeText"/>
                            <instructionText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldDescription="Please enter the Instruction text"
                                fieldLabel="Instruction Text"
                                name="./instructionText"/>
                        </items>
                    </welcomeConfig>
                    <agRewards
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Ag Rewards Section"
                        sling:resourceType="granite/ui/components/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <agRewardsTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter title the for AG Rewards block"
                                fieldLabel="AG Rewards Title"
                                name="./agRewardsTitle"/>
                            <membershipNumberLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter Membership Number Label"
                                fieldLabel="Membership Number Label"
                                name="./membershipNumberLabel"/>
                            <rewardPoints
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter Reward Points Label"
                                fieldLabel="Reward Points Label"
                                name="./rewardPoints"/>
                            <rewardTier
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter Reward Tier Label"
                                fieldLabel="Reward Tier Label"
                                name="./rewardTier"/>
                            <dollarsOfNextTier
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter Dollars of Next Tier Label"
                                fieldLabel="Dollars of Next Tier Label"
                                name="./dollarsOfNextTier"/>
                            <tierExpDate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter Tier Expiration Date Label"
                                fieldLabel="Tier Expiration Date Label"
                                name="./tierExpDate"/>
                            <addReceiptsAndMoreTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter title the for Add Receipts And More block"
                                fieldLabel="Add Receipts and More Title"
                                name="./addReceiptsAndMoreTitle"/>
                            <addReceiptsAndMoreExplanation
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter Add Receipts and More Explanation Label"
                                fieldLabel="Add Receipts and More Explanation Label"
                                name="./addReceiptsAndMoreExplanation"/>
                            <addNowLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter Add Now Label"
                                fieldLabel="Add Now Label"
                                name="./addNowLabel"/>
                            <addNowLabelCta
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Please enter the link for add receipts and more"
                                fieldLabel="CTA link for Add receipts and more"
                                name="./addNowLabelCta"
                                rootPath="/content"/>
                            <emailAddressLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter title the for AG Rewards Email block"
                                fieldLabel="AG Rewards  Email Address Title"
                                name="./emailAddressLabel"/>
                            <editButtonLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter Edit Button Label"
                                fieldLabel="Edit Button Label"
                                name="./editButtonLabel"/>
                        </items>
                    </agRewards>
                    <defaultAddressNShipment
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Default Address &amp; Shipment"
                        sling:resourceType="granite/ui/components/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <defaultAddressTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Default Address"
                                fieldLabel="Default Address Label "
                                name="./defaultAddressTitle"/>
                            <addNewAddress
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Add New Address"
                                fieldLabel="Add New Address Label "
                                name="./addNew Address"/>
                            <editAddress
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Edit Address"
                                fieldLabel="Edit Address Label "
                                name="./editAddress"/>
                            <editrAddAddressCta
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Please enter the link for Edit/Add New Address"
                                fieldLabel="CTA link for Edit/Add New Address"
                                name="./editrAddAddressCta"
                                rootPath="/content/ag/en/shop/home/my-account/"/>
                            <defaultShipping
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Default Shipping Method"
                                fieldLabel="Default Shipping Method Label "
                                name="./defaultShippingMethod"/>
                            <addNewShipping
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Add New Shipping Method"
                                fieldLabel="Add New Shipping Method Label "
                                name="./addNewShipping"/>
                            <editShipping
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Edit Shipping Method"
                                fieldLabel="Edit Shipping Method Label "
                                name="./editShipping"/>
                            <editrAddShippingCta
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Please enter the link for Edit/Add New Shipping Method"
                                fieldLabel="CTA link for Edit/Add New Shipping Method"
                                name="./editrAddShippingCta"
                                rootPath="/content/ag/en/shop/home/my-account"/>
                            <defaultPayment
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Default Payment Method"
                                fieldLabel="Default Payment Option Label "
                                name="./defaultPayment"/>
                            <addCrDCard
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Add New Credit/Debit Card"
                                fieldLabel="Add New Credit/Debit Card Label"
                                name="./addNewCrDCard"/>
                            <editCrDCard
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Please enter the Label for Credit/Debit Card"
                                fieldLabel="Edit Credit/Debit Card Label"
                                name="./editCrDCard"/>
                            <editrAddCrDCardCta
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Please enter the link for Edit/Add New Credit/Debit Card"
                                fieldLabel="CTA link for Edit/Add New Credit/Debit Card"
                                name="./editrAddCrDCardCta"
                                rootPath="/content/ag/en/shop/home/my-account"/>
                        </items>
                    </defaultAddressNShipment>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
