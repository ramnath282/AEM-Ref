<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Return Form"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <returnShipping
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Return Shipping Form"
                        sling:resourceType="granite/ui/components/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <firstNameLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="First Name Label"
                                name="./firstNameLabel"
                                required="{Boolean}true"/>
                            <lastNameLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Last Name Label"
                                fieldLabel="Last Name Label"
                                name="./lastNameLabel"
                                required="{Boolean}true"/>
                            <stateLable
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="State Label"
                                fieldLabel="State Lable"
                                name="./stateLabel"
                                required="{Boolean}true"/>
                            <zipCode
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="ZIP Code Label"
                                fieldLabel="ZIP Code Lable"
                                name="./zipCodeLabel"
                                required="{Boolean}true"/>
                            <ctaText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="CTA Button Label"
                                fieldLabel="CTA Button Label"
                                name="./ctaButtonLabel"
                                required="{Boolean}true"/>
                            <isGT
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}false"
                                name="./isGt"
                                text="Is GT?"
                                uncheckedValue="N"
                                value="true"/>
                        </items>
                    </returnShipping>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
