<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Create An Account"
          sling:resourceType="cq/gui/components/authoring/dialog"
          height="{Long}500"
          width="{Long}600">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <descriptionText
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Description Text Section"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the title"
                                            fieldLabel="Title:"
                                            name="./title"/>
                                    <descriptionHeading
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the Description Heading"
                                            fieldLabel="Description Heading:"
                                            name="./descriptionHeading"/>
                                    <descriptionPoints
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            composite="{Boolean}true"
                                            fieldLabel="Description Points">
                                        <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                name="./descriptionMultifield">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <descriptionPoint
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter a description point"
                                                                fieldLabel="Description Point:"
                                                                name="./descriptionPoint"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                        <granite:data jcr:primaryType="nt:unstructured"/>
                                    </descriptionPoints>
                                </items>
                            </column>
                        </items>
                    </descriptionText>
                    <formFields
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Form Fields Section"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <createAccountButtonLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Create New Account Button"
                                            fieldLabel="Create New Account Button Label:"
                                            name="./createAccountButtonLabel"/>
                                    <firstNameLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the First Name Input Box "
                                            fieldLabel="First Name Label:"
                                            name="./firstNameLabel"/>
                                    <lastNameLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Last Name Input Box "
                                            fieldLabel="Last Name Label:"
                                            name="./lastNameLabel"/>
                                    <emailAddressLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Email Address Input Box "
                                            fieldLabel="Email Address Label:"
                                            name="./emailAddressLabel"/>
                                    <verifyEmailAddressLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Verify Email Address Input Box "
                                            fieldLabel="Verify Email Address Label:"
                                            name="./verifyEmailAddressLabel"/>
                                    <passwordLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Password Input Box "
                                            fieldLabel="Password Label:"
                                            name="./passwordLabel"/>
                                    <verifyPasswordLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Verify Password Input Box "
                                            fieldLabel="Verify Password Label:"
                                            name="./verifyPasswordLabel"/>
                                    <phoneNumberLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Phone Number Input Box "
                                            fieldLabel="Phone Number Label:"
                                            name="./phoneNumberLabel"/>
                                    <dobTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter title for DOB section"
                                            fieldLabel="DOB section title:"
                                            name="./dobTitle"/>
                                    <dobDateLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the label for Birth Date"
                                            fieldLabel="Birth Date Label:"
                                            name="./dobDateLabel"/>
                                    <dobMonthLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the label for Birth Month"
                                            fieldLabel="Birth Month Label:"
                                            name="./dobMonthLabel"/>
                                    <dobYearLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the label for Birth Year"
                                            fieldLabel="Birth Year Label:"
                                            name="./dobYearLabel"/>
                                    <rewardsText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for AG Rewards Program checkbox"
                                            fieldLabel="AG Rewards Program text:"
                                            name="./rewardsText"/>
                                    <tcLinkURL
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Please enter URL for Terms &amp; Conditions Link"
                                            fieldLabel="Terms &amp; Conditions Link URL:"
                                            name="./tcLinkURL"/>
                                    <tcLinkText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for Terms &amp; Conditions link"
                                            fieldLabel="Terms &amp; Conditions link text:"
                                            name="./tcLinkText"/>
                                    <promotionsText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for AG Promotions and Events checkbox"
                                            fieldLabel="AG Promotions and Events text:"
                                            name="./promotionsText"/>
                                    <priavacyLinkURL
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Please enter URL for Privacy Statement Link"
                                            fieldLabel="Privacy Statement Link URL:"
                                            name="./priavacyLinkURL"/>
                                    <privacyLinkText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for Privacy Statement link"
                                            fieldLabel="Privacy Statement link text:"
                                            name="./privacyLinkText"/>
                                    <cancelButtonLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Cancel Button "
                                            fieldLabel="Cancel Button Label:"
                                            name="./cancelButtonLabel"/>
                                    <usCustomersOnlyText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the text for United States Customers only notification"
                                            fieldLabel=" US Customers only notification text:"
                                            name="./usCustomersOnlyText"/>
                                    <usBillingAddressOnlyText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the text for United States Billing Addresses only notification"
                                            fieldLabel=" US Billing Addresses only notification text:"
                                            name="./usBillingAddressOnlyText"/>
                                    <websiteUseTcLinkURL
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Please enter URL for Website Use Terms &amp; Conditions Link"
                                            fieldLabel="Website Use Terms &amp; Conditions Link URL:"
                                            name="./websiteUseTcLinkURL"/>
                                    <websiteUseTcLinkText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for Website Use Terms &amp; Conditions"
                                            fieldLabel="Website Use Terms &amp; Conditions text:"
                                            name="./websiteUseTcLinkText"/>
                                    <okButtonLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the OK Button "
                                            fieldLabel="OK Button Label:"
                                            name="./okButtonLabel"/>
                                    <successpath
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldDescription="Please enter the Success Path"
                                            fieldLabel="Success Path"
                                            name="./successPath"/>
                                </items>
                            </column>
                        </items>
                    </formFields>
                    <errorModal
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Error Modal Section"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <redirectPathForErrorModal
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldDescription="Please enter the Redirect Path for Error Modal"
                                            fieldLabel="Redirect Path for Error Modal"
                                            name="./redirectPathForErrorModal"/>
                                    <modalText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                            fieldDescription="Please enter Error Modal Text"
                                            fieldLabel="Error Text"
                                            name="./modalText"
                                            useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="mattel/global/components/content/rtePlugins/rtePlugins"/>
                                        <uiSettings
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="mattel/global/components/content/rtePlugins/uiSettings"/>
                                    </modalText>
                                    <okButtonText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please Ok Button text"
                                            fieldLabel="Ok Button text"
                                            name="./okButtonText"/>
                                </items>
                            </column>
                        </items>
                    </errorModal>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
