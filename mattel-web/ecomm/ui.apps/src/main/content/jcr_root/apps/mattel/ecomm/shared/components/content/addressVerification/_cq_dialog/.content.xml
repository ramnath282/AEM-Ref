<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Address Book"
          sling:resourceType="cq/gui/components/authoring/dialog"
          width="{Long}600">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <addressVerificationSection
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Address Verification Section"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <adrsVerificationTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter Title for Address Verification Section"
                                            fieldLabel="Address Verification Section Title :"
                                            name="./adrsVerificationSectionTitle"/>
                                    <informativeDisclamier
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                            fieldDescription="Please enter Label for Informative Disclaimer For Address Verification"
                                            fieldLabel="Informative Disclaimer Label :"
                                            name="./informativeDisclamier"
                                            useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="mattel/global/components/content/rtePlugins/rtePlugins"/>
                                        <uiSettings
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="mattel/global/components/content/rtePlugins/uiSettings"/>
                                    </informativeDisclamier>
                                    <adrsEnteredSectionTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter Title for the Entered Address Section"
                                            fieldLabel="Entered Address Section Title :"
                                            name="./adrsEnteredSectionTitle"/>
                                    <informativeDisclamierForEnteredAddSection
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter Label for Informative Disclaimer For Entered Address Section"
                                            fieldLabel="Informative Disclaimer Label For Entered Address Section :"
                                            name="./informativeDisclamierForEnteredAddSection"/>
                                    <ctaForEnteredAddrSectionTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter Label for CTA For Entered Address Section"
                                            fieldLabel="CTA Label For Entered Address Section:"
                                            name="./ctaForEnteredAddrSectionTitle"/>
                                    <useThisAddressLinkLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter Cta Label for Use This Address"
                                            fieldLabel="Cta Label for Use This Address"
                                            name="./useThisAddressLinkLabel"/>
                                    <uSPSRecommendedAddrSectionTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter Title for the USPS Recommended Address Section"
                                            fieldLabel="USPS Recommended Address Section Title :"
                                            name="./uSPSRecommendedAddrSectionTitle"/>
                                    <uspsNotFoundErrorMessage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                            fieldDescription="Usps Not Found Message"
                                            fieldLabel="Usps Not found Error message"
                                            name="./uspsNotFoundErrorMessage"
                                            useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="mattel/global/components/content/rtePlugins/rtePlugins"/>
                                        <uiSettings
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="mattel/global/components/content/rtePlugins/uiSettings"/>
                                    </uspsNotFoundErrorMessage>
                                    <ctaForUSPSRecommendedAddrSectionTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter Label for CTA For USPS Recommended Address Section"
                                            fieldLabel="CTA Label For USPS Recommended Address Section:"
                                            name="./ctaForUSPSRecommendedAddrSectionTitle"/>
                                </items>
                            </column>
                        </items>
                    </addressVerificationSection>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
