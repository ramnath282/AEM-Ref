<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Create An Account"
          sling:resourceType="cq/gui/components/authoring/dialog"
          height="{Long}500"
          width="{Long}600">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <productInterestHeaderLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Product Interest Header Label"
                            fieldLabel="Product Interest Header Label"
                            name="./productInterestHeaderLabel"/>
                    <storeInterestHeaderLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Please enter header label for store interest"
                            fieldLabel="Store Interest Header Label"
                            name="./storeInterestHeaderLabel"/>
                    <storeListDefaultOption
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Please enter store list default option"
                            fieldLabel="Store List Default Option"
                            name="./storeListDefaultOption"/>
                    <updateInterestButtonText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Please enter update interest button text"
                            fieldLabel="Update Interest Button Text"
                            name="./updateInterestButtonText"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
