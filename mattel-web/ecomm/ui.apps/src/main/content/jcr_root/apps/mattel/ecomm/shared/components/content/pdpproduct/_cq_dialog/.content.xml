<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="PDP Product Component"
    sling:resourceType="cq/gui/components/authoring/dialog"
    height="{Long}500"
    width="{Long}600">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <mainsection
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Main Section"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <addtobagbuttontext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Add to Bag Button Text"
                                        fieldLabel="Add to Bag Button Label"
                                        name="./addtobagButtonText"/>
                                    <checkstoreavailabilitytext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter text for Check Store Availability"
                                        fieldLabel="Check Store Availability Label"
                                        name="./checkStoreAvailabilityText"/>
                                    <marketingagelabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Marketing Age Label"
                                        fieldLabel="Marketing Age Label"
                                        name="./marketingAgeLabel"/>
                                    <readreviewlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Read-Review Label Text"
                                        fieldLabel="Read Review Label"
                                        name="./readReviewLabel"/>
                                    <writereviewlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Write-Review Label Text"
                                        fieldLabel="Write-Review Label"
                                        name="./writeReviewLabel"/>
                                    <availabilitystatusline
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Availability Status Line Label Text"
                                        fieldLabel=" Availability Status Line Label"
                                        name="./availabilityStatusLine"/>
                                    <inventorystatusline
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Inventory Status Line Label Text"
                                        fieldLabel="Inventory Status Line Label"
                                        name="./inventoryStatusLine"/>
                                    <ourguaranteetitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Our Guarantee Title Text"
                                        fieldLabel="Our Guarantee Title Text"
                                        name="./ourGuaranteeTitle"/>
                                    <ourguaranteesubtitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Our Guarantee Subtitle Text"
                                        fieldLabel="Our Guarantee Subtitle Text"
                                        name="./ourGuaranteeSubtitle"/>
									<ourguaranteeDetailtitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Our Guarantee Detail Title which shows on popop"
                                        fieldLabel="Our Guarantee Detail Title Text"
                                        name="./ourguaranteeDetailtitle"/>
                                    <ourguaranteedescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldDescription="Please enter Our Guarantee Description Text"
                                        fieldLabel="Our Guarantee Description Text"
                                        name="./ourGuaranteeDescription"
										useFixedInlineToolbar="{Boolean}true">
										<rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="mattel/global/components/content/rtePlugins/rtePlugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="mattel/global/components/content/rtePlugins/uiSettings"/>
									</ourguaranteedescription>
                                    <pricestrickmessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Price strikethrough Message"
                                        fieldLabel="Price strikethrough Message"
                                        name="./pricestrickMessage"/>
                                    <yousavemessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter You Save ( Discount ) Message"
                                        fieldLabel="You Save ( Discount ) Message"
                                        name="./yousaveMessage"/>
									<checkboxErrorMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter error message to be displayed when user clicks on Add to Bag without selecting any product checkbox"
                                        fieldLabel="Select Checkbox Error Message"
                                        name="./checkboxErrorMessage"/>
									<excludeShippingCountryMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Exclude Shipping Country Message. For e.g. Cannot ship to Canada"
                                        fieldLabel="Exclude Shipping Country Message"
                                        name="./excludeShippingCountryMessage"/>
									<excludeShippingCountryMessageForSku
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter a detailed descriptionwhen an altCanada sku IS applied"
                                        fieldLabel="Message to display if an altCanada sku IS applied"
                                        name="./excludeShippingCountryMessageForSku"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                    <affirmLearnMoreLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Affirm Learn More Label Text"
                                        fieldLabel="Affirm Learn More Label"
                                        name="./affirmLearnMoreLabel"/>
                                    <affirmLearnMoreLink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                        fieldDescription="Please enter affirm learn more page path"
                                        fieldLabel="Affirm Learn More Page Link"
                                        name="./affirmLearnMoreLink"/>
                                </items>
                            </column>
                        </items>
                    </mainsection>
                    <statickit
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Static Kit/Style Header Section"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <sizeLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Label for Size"
                                        fieldLabel="Size Label :"
                                        name="./sizeLabel"/>
                                    <colorLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Label for Color"
                                        fieldLabel="Color Label :"
                                        name="./colorLabel"/>
                                    <selectswaatcherrormessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter error message to be displayed when user does not select size swatch"
                                        fieldLabel="Size Swatch Error Message"
                                        name="./selectSwatchErrorMessage"/>
                                    <selectcolorerrormessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter error message to be displayed when user does not select color swatch"
                                        fieldLabel="Color Swatch Error Message"
                                        name="./selectColorMessage"/>
                                    <sizechartlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Label for Size Chart"
                                        fieldLabel="Size Chart Message Label"
                                        name="./sizeChartLabel"/>
                                </items>
                            </column>
                        </items>
                    </statickit>
                    <bittyTwinsSection
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Biity Twins Section"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <bittyBabyChoice1Label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter text for Bitty Baby Choice 1 which will be shown on top of Dolls"
                                        fieldLabel="Bitty Baby Choice 1 Label"
                                        name="./bittyBabyChoice1Label"/>
                                    <sleeperChoice1Label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Label Sleeper Choice 1 text shown on top of sleepers section"
                                        fieldLabel="Sleeper Choice 1 Label"
                                        name="./sleeperChoice1Label"/>
                                    <bittyBabyChoice2Label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter text for Bitty Baby Choice 2 which will be shown on top of Dolls"
                                        fieldLabel="Bitty Baby Choice 2 Label"
                                        name="./bittyBabyChoice2Label"/>
                                    <sleeperChoice2Label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Sleeper Choice 2 text shown on top of sleepers section"
                                        fieldLabel="Sleeper Choice 2 Label"
                                        name="./sleeperChoice2Label"/>
                                </items>
                            </column>
                        </items>
                    </bittyTwinsSection>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
