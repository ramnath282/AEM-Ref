<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Order History Detail"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <orderDetailHeading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Order Detail Heading"
                        fieldLabel="Order Detail Heading"
                        name="./orderDetailHeading"/>
                    <backToOrderHistoryLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Back to order History Label"
                        fieldLabel="Back to order History Label"
                        name="./backToOrderHistoryLabel"/>
                    <backToOrderHistroyLink
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldDescription="Please select the link for Back To Order History"
                        fieldLabel="Back To Order History Link"
                        name="./backToOrderHistoryLink"
                        rootPath="/content"/>
                    <orderDateLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Order Date Label"
                        fieldLabel="Order Date Label"
                        name="./orderDateLabel"/>
                    <orderNumberLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Order Number Label"
                        fieldLabel="Order Number Label"
                        name="./orderNumberLabel"/>
                    <orderStatusLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Order Status Label"
                        fieldLabel="Order Status Label"
                        name="./orderStatusLabel"/>
                    <orderTotalLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Order Total Label"
                        fieldLabel="Order Total Label"
                        name="./orderTotalLabel"/>
                    <defaultProductThumbnail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                        allowUpload="{Boolean}false"
                        autoStart="{Boolean}false"
                        class="cq-droptarget"
                        fieldLabel="Default Product Thumbnail Image"
                        fileReferenceParameter="./defaultProductThumbnail"
                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                        multiple="{Boolean}false"
                        name="./defaultProductThumbnailImage"
                        uploadUrl="${suffix.path}"
                        useHTML5="{Boolean}true"/>
                    <productLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Product Label"
                        fieldLabel="Product Label"
                        name="./productLabel"/>
                    <statusLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Status Label"
                        fieldLabel="Status Label"
                        name="./statusLabel"/>
                    <quantityLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Quantity Label"
                        fieldLabel="Quantity Label"
                        name="./quantityLabel"/>
                    <eachLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Each Label"
                        fieldLabel="Each Label"
                        name="./eachLabel"/>
                    <totalLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Total Label"
                        fieldLabel="Total Label"
                        name="./totalLabel"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
