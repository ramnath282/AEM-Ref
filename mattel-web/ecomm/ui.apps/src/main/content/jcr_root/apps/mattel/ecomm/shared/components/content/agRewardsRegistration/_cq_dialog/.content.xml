<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Create An Account"
          sling:resourceType="cq/gui/components/authoring/dialog"
          height="{Long}500"
          width="{Long}600">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <descriptionText
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Description Text Section"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <queryToMember
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the Query to Member"
                                            fieldLabel="Query to Member:"
                                            name="./queryToMember"/>
                                    <suggestionToMember
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the Suggestion to Member"
                                            fieldLabel="Suggestion to Member:"
                                            name="./suggestionToMember"/>
                                </items>
                            </column>
                        </items>
                    </descriptionText>
                    <formFields
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Form Fields Section"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <linkAgRewardsLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Link AG Rewards Membership"
                                            fieldLabel="Link AG Rewards Membership Label:"
                                            name="./linkAgRewardsLabel"/>
                                    <cancelButtonLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Cancel Button "
                                            fieldLabel="Cancel Button Label:"
                                            name="./cancelButtonLabel"/>
                                    <membershipInfoDescription
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the Membership Info Description"
                                            fieldLabel="Membership Info Description:"
                                            name="./membershipInfoDesc"/>
                                    <agRewardsMembershipIdLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for AG Rewards Membership ID"
                                            fieldLabel="AG Rewards Membership ID Label:"
                                            name="./agRewardsMembershipIdLabel"/>
                                    <emailAddressLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Email Address Input Box "
                                            fieldLabel="Email Address Label:"
                                            name="./emailAddressLabel"/>
                                    <findAgRewardsMembershipLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter Label for Find AG Rewards Membership"
                                            fieldLabel="Find AG Rewards Membership Label:"
                                            name="./findAgRewardsMembership"/>
                                    <passwordLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Password Input Box "
                                            fieldLabel="Password Label:"
                                            name="./passwordLabel"/>
                                    <verifyPasswordLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Verify Password Input Box "
                                            fieldLabel="Verify Password Label:"
                                            name="./verifyPasswordLabel"/>
                                    <privacyAgreeText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for Privacy Agreement"
                                            fieldLabel="Privacy Agreement text:"
                                            name="./privacyAgreementText"/>
                                    <privacyLinkURL
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Please enter URL for Privacy Statement Link"
                                            fieldLabel="Privacy Statement Link URL:"
                                            name="./privacyLinkURL"/>
                                    <privacyLinkText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for Privacy Statement link"
                                            fieldLabel="Privacy Statement link text:"
                                            name="./privacyLinkText"/>
                                    <dobTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter title for DOB section"
                                            fieldLabel="DOB section title:"
                                            name="./dobTitle"/>
                                    <dobDateLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the label for Birth Date"
                                            fieldLabel="Birth Date Label:"
                                            name="./dobDateLabel"/>
                                    <dobMonthLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the label for Birth Month"
                                            fieldLabel="Birth Month Label:"
                                            name="./dobMonthLabel"/>
                                    <dobYearLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter the label for Birth Year"
                                            fieldLabel="Birth Year Label:"
                                            name="./dobYearLabel"/>
                                    <createAccountButtonLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter label for the Create New Account Button"
                                            fieldLabel="Create New Account Button Label:"
                                            name="./createAccountButtonLabel"/>
                                    <emailExistsMessage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for Email Already Exits Message"
                                            fieldLabel="Email already exits Message:"
                                            name="./emailExistsMessage"/>
                                    <successMessage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Please enter text for the AG Rewards Account Located Message"
                                            fieldLabel="AG Rewards Account Located Message:"
                                            name="./agRewardsAccMessage"/>
                                    <successpath
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldDescription="Please enter the Success Path"
                                            fieldLabel="Success Path"
                                            name="./successPath"/>
                                </items>
                            </column>
                        </items>
                    </formFields>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
