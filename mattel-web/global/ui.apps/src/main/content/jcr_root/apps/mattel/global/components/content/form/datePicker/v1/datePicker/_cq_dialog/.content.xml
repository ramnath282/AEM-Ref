<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Date Picker"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <configTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <viewer
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Background Configuration"
                                sling:resourceType="granite/ui/components/foundation/section">
                                <config
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Config"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <elementName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Element Name"
                                                name="./elementName"
                                                required="true"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./title"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Description"
                                                name="./description"/>
                                            <formatType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Format"
                                                name="./formatType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <mmddyyyy
                                                        jcr:primaryType="nt:unstructured"
                                                        text="MM-DD-YYYY"
                                                        value="mm-dd-yyyy"/>
                                                    <ddmmyyyy
                                                        jcr:primaryType="nt:unstructured"
                                                        text="DD-MM-YYYY"
                                                        value="dd-mm-yyyy"/>
                                                    <yyyymmdd
                                                        jcr:primaryType="nt:unstructured"
                                                        text="YYYY-MM-DD"
                                                        value="yyyy-mm-dd"/>
                                                </items>
                                            </formatType>
                                        </items>
                                    </column>
                                </items>
                            </viewer>
                        </items>
                    </configTab>
                    <initialValue
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Placeholder Text"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <viewer
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Background Configuration"
                                sling:resourceType="granite/ui/components/foundation/section">
                                <config
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Config"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <placeHolderText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Consumer seeing text"
                                                fieldLabel="PlaceHolder Text"
                                                name="./placeHolderText"/>
                                        </items>
                                    </column>
                                </items>
                            </viewer>
                        </items>
                    </initialValue>
                    <constraints
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Constraints"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <viewer
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Background Configuration"
                                sling:resourceType="granite/ui/components/foundation/section">
                                <config
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Config"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <required
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldLabel="Required"
                                                name="./required"
                                                text="Required"
                                                uncheckedValue="false"
                                                value="true"/>
                                            <requiredMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Required Message"
                                                name="./requiredMessage"/>
                                        </items>
                                    </column>
                                </items>
                            </viewer>
                        </items>
                    </constraints>
					<tooltipConfiguration
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tooltip Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <viewer
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Tooltip Configuration"
                                sling:resourceType="granite/ui/components/foundation/section">
                                <config
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Config"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <tooltipMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Enter Tooltip Message for the field"
                                                fieldLabel="Tooltip Message"
                                                name="./tooltipMessage"/>
                                        </items>
                                    </column>
                                </items>
                            </viewer>
                        </items>
                    </tooltipConfiguration>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
