    jcr:primaryType="nt:unstructured"
    jcr:title="Informational Footer"
    sling:resourceType="cq/gui/components/authoring/dialog"
    height="{Long}500">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <backgroundStyle
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Background Style"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <backgroundColor
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                        fieldLabel="Background Color:"
                                        name="./backgroundColor"/>
                                    <backgroundImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Upload Background Image:"
                                        fileReferenceParameter="./fileReferenceBgImg"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                        multiple="{Boolean}false"
                                        name="./backgroundImage"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <backgroundoption
                                        granite:class="cq-dialog-dropdown-showhide"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Background Option"
                                        name="./backOption">
                                        <items jcr:primaryType="nt:unstructured">
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                text="Image"
                                                value="image"/>
                                            <color
                                                jcr:primaryType="nt:unstructured"
                                                text="Color"
                                                value="color"/>
                                        </items>
                                    </backgroundoption>
                                    <alwaysEnglish
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Always English check for Multi-Lingual  Site Tagging"
                                        fieldLabel="Adobe Tracking Name"
                                        name="./alwaysEnglish"
                                        required="{Boolean}true"/>
                                    <aeLocationName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Location Name for Analytics (Always English )"
                                        fieldLabel="Location Name for Adobe Tracking"
                                        name="./locationName"/>
                                </items>
                            </column>
                        </items>
