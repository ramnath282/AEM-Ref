                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Same Window"
                                                                    value="sameWindow"/>
                                                                <newTab
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="New Tab"
                                                                    value="tabWindow"/>
