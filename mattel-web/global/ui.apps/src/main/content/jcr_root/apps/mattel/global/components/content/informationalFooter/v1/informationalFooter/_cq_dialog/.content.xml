                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="New Window"
                                                                    value="newWindow"/>
                                                                <interstitalLeaving
                                                                    granite:hide="${cqDesign.disableSearch}"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Interstitial Leaving The Site"
                                                                    value="interstitialLeaving"/>
                                                            </items>
